{
  "env": {
    "CLAUDE_CODE_TMPDIR": ".claude/tmpdir"
  },
  "permissions": {
    "allow": [
      "Bash",
      "Read",
      "Write",
      "Glob",
      "Grep",
      "Edit",
      "WebSearch",
      "WebFetch",
      "Task",
      "KillShell",
      "NotebookEdit",
      "Skill",
      "TodoWrite",
      "mcp__ide__getDiagnostics",
      "mcp__plugin_github_github__*",
      "mcp__pycharm2__",
      "mcp__pycharm__"
    ]
  },
  "enabledPlugins": {
    "frontend-design@claude-plugins-official": true,
    "frontend-design@claude-code-plugins": true
  }
}
